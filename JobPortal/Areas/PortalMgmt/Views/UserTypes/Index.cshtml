@model IEnumerable<JobPortal.Models.UserType>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table table-striped table-hover table-bordered">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserTypeName)
            </th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserTypeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UserTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UserTypeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UserTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
